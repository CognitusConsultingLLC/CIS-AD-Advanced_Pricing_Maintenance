{
    "_version": "1.8.0",
    "sap.app": {
        "id": "CGDC.CIS-AD-Pricing-Maintenance",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "tags": {
            "keywords": []
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/cgdc/UI_PRICING_MAINT/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "_CGDC_UI_PRICING_MAINT_VAN"
                    ],
                    "localUri": "localService/metadata.xml"
                }
            },
            "_CGDC_UI_PRICING_MAINT_VAN": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='%2FCGDC%2FUI_PRICING_MAINT_VAN',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/_CGDC_UI_PRICING_MAINT_VAN.xml"
                }
            },
            "CCHDR" :{
                "uri": "/sap/opu/odata/sap/YUI_CC_HDR/",
				"type": "OData"
            }
        },
        "offline": false,
        "sourceTemplate": {
            "id": "ui5template.smartTemplate",
            "version": "1.40.12",
            "toolsId": "83fb6c3e-1b4a-4089-8e40-d45f19180c61"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "supportedThemes": [
            "sap_fiori_3"
        ]
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.108.26",
            "libs": {
                "sap.ui.core": {
                    "lazy": false
                },
                "sap.ui.generic.app": {
                    "lazy": false
                },
                "sap.suite.ui.generic.template": {
                    "lazy": false
                }
            },
            "components": {}
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ListReport|xCGDCxI_PRICING_MAIN": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ListReport/xCGDCxI_PRICING_MAIN/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|xCGDCxI_PRICING_MAIN": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/xCGDCxI_PRICING_MAIN/i18n.properties"
            },
            "i18n|sap.suite.ui.generic.template.ObjectPage|xCGDCxI_CONDITON_CATALOG": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/ObjectPage/xCGDCxI_CONDITON_CATALOG/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    }
                }
            },
            "customFieldsModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "OneWay",
                    "defaultCountMode": "Request"
                },
                "preload": true
            },
            "CustomFields": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "OneWay",
                    "defaultCountMode": "Request"
                },
                "preload": true
            },
            "demoapp": {
                "type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "CCHDR",
				"preload": true

            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.suite.ui.generic.template.ObjectPage.view.Details": {
                        "controllerName": "CGDC.CIS-AD-Pricing-Maintenance.ext.controller.ObjectPageExt",
                        "sap.ui.generic.app": {
                            "xCGDCxI_CONDITON_CATALOG": {
                                "EntitySet": "xCGDCxI_CONDITON_CATALOG",
                                "Sections":{
                                    "ItemDetails":{
                                        "id" :"ItemDetails",
                                        "Actions" :{
                                            "ImportUpload" :{
                                                "id" : "ImportUpload",
                                                "text" : "{@i18n>import}",
                                                "press" : "onImportToExcel",
                                                "requiresSelection": false
                                            },
                                            "ExcelUpload" :{
                                                "id" : "ExcelUpload",
                                                "text" : "{@i18n>upload}",
                                                "press" : "onExcelUpload",
                                                "requiresSelection": false
                                            },
                                            "Groups" :{
                                                "id" : "Groups",
                                                "text" : "{@i18n>groups}",
                                                "press" : "onPressGrops",
                                                "requiresSelection": false
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "settings": {
            "flexibleColumnLayout": {
                "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
            },
            "forceGlobalRefresh": true,
            "objectPageHeaderType": "Dynamic",
            "showDraftToggle": false
        },
        "pages": {
            "ListReport|xCGDCxI_PRICING_MAIN": {
                "entitySet": "xCGDCxI_PRICING_MAIN",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "filterSettings": {
                            "dateSettings": {
                                "useDateRange": true
                            }
                        },
                        "smartVariantManagement": true,
                        "condensedTableLayout": true
                    }
                },
                "pages": {
                    "ObjectPage|xCGDCxI_PRICING_MAIN": {
                        "entitySet": "xCGDCxI_PRICING_MAIN",
                        "defaultLayoutType": "OneColumn",
                        "component": {
                            "name": "sap.suite.ui.generic.template.ObjectPage"
                        },
                        "pages": {
                            "ObjectPage|toCondCat": {
                                "navigationProperty": "toCondCat",
                                "entitySet": "xCGDCxI_CONDITON_CATALOG",
                                "defaultLayoutType": "OneColumn",
                                "defaultLayoutTypeIfExternalNavigation": "MidColumnFullScreen",
                                "component": {
                                    "name": "sap.suite.ui.generic.template.ObjectPage"
                                },
                                "pages": {
                                    "ObjectPage|toCncMain": {
                                        "navigationProperty": "toCncMain",
                                        "entitySet": "xCGDCxI_CNC_MAIN",
                                        "defaultLayoutTypeIfExternalNavigation": "MidColumnFullScreen",
                                        "component": {
                                            "name": "sap.suite.ui.generic.template.ObjectPage"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "sap.platform.hcp": {
        "uri": ""
    },
    "sap.platform.abap": {
        "uri": "/sap/bc/ui5_ui5/cgdc/fradvprcing/webapp",
        "_version": "1.1.0"
    }
}